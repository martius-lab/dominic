{
  "optimization_procedure_name": "test",
  "results_dir": "results",
  "script_relative_path": "solo_legged_gym/cluster/train_cluster.py",
  "git_params": {
  },
  "environment_setup": {
    "run_as_module": false
  },
  "cluster_requirements": {
    "request_cpus": 1,
    "request_gpus": 1,
    "memory_in_mb": 10000,
    "cuda_requirement": "TARGET.CUDACapability >= 6.0",
    "bid": 20,
    "forbidden_hostnames": []
  },
  "fixed_params": {
    "__import_promise__": "solo_legged_gym/envs/solo12_domino/solo12_domino.json"
  },

  "remove_jobs_dir": false,
  "run_in_working_dir": true,
  "local_run": false,
  "restarts": 1,
  "hyperparam_list": [
    {
     "param": "solo_legged_gym.train_cfg.algorithm.lagrange_learning_rate",
     "values": [5e-4, 1e-3, 5e-3]
    },
    {
     "param": "solo_legged_gym.train_cfg.algorithm.sigmoid_scale",
     "values": [1.0, 5.0]
    },
    {
     "param": "solo_legged_gym.train_cfg.algorithm.fixed_rew_scale",
     "values": [1.0, 0.5, 0.3, 0.2, 0.1, 0.05]
    },
    {
     "param": "solo_legged_gym.train_cfg.algorithm.scale_fixed_advantages",
     "values": [true, false]
    }
  ]
}
