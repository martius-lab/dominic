{
  "optimization_procedure_name": "test",
  "results_dir": "results",
  "script_relative_path": "solo_legged_gym/cluster/train_cluster.py",
  "git_params": {
  },
  "environment_setup": {
    "run_as_module": false
  },
  "cluster_requirements": {
    "request_cpus": 1,
    "request_gpus": 1,
    "memory_in_mb": 10000,
    "gpu_memory_mb": 12000,
    "cuda_requirement": "TARGET.CUDACapability >= 6.0",
    "bid": 15,
    "forbidden_hostnames": ["g095", "g023", "g020", "g097"]
