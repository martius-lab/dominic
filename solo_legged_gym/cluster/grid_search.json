  "remove_jobs_dir": false,
  "run_in_working_dir": true,
  "local_run": false,
  "restarts": 1,
  "hyperparam_list": [
    {
      "param": "solo_legged_gym.env_cfg.rewards.terms.dof_acc",
      "values": ["[0, 2000]", "[0, 3000]", "[0, 4000]"]
    },
    {
      "param": "solo_legged_gym.env_cfg.init_state.init_outside",
      "values": [false, true]
    },
    {
      "param": "solo_legged_gym.env_cfg.control.torque_limits",
      "values": [2.5, 5.0, 7.5, 10.0]
    }
  ]
