{
  "optimization_procedure_name": "lr",
  "results_dir": "results",
  "script_relative_path": "solo_legged_gym/cluster/train_cluster.py",
  "git_params": {
  },
  "environment_setup": {
    "run_as_module": false
  },
  "cluster_requirements": {
    "request_cpus": 1,
    "request_gpus": 1,
    "memory_in_mb": 10000,
    "gpu_memory_mb": 18000,
    "cuda_requirement": "TARGET.CUDACapability >= 6.0",
    "bid": 15,
    "forbidden_hostnames": []
  },
  "fixed_params": {
    "__import_promise__": "solo_legged_gym/envs/solo12_domino_position/solo12_domino_position.json"
  },

  "remove_jobs_dir": false,
  "run_in_working_dir": true,
  "local_run": false,
  "restarts": 1,
  "hyperparam_list": [
    {
      "param": "solo_legged_gym.train_cfg.algorithm.value_lr",
      "values": [1e-3, 3e-3, 5e-3]
    },
    {
      "param": "solo_legged_gym.train_cfg.algorithm.policy_lr",
      "values": [5e-4, 1e-3]
    },
      {
      "param": "solo_legged_gym.train_cfg.algorithm.lagrange_lr",
      "values": [5e-4, 1e-3, 3e-3]
    },
    {
      "param": "solo_legged_gym.env_cfg.seed",
      "values": [22, 27, 42]
    }
  ]
}
