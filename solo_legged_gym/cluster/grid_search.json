{
  "optimization_procedure_name": "test",
  "results_dir": "results",
  "script_relative_path": "solo_legged_gym/cluster/train_cluster.py",
  "git_params": {
  },
  "environment_setup": {
    "run_as_module": false
  },
  "cluster_requirements": {
    "request_cpus": 1,
    "request_gpus": 1,
    "memory_in_mb": 10000,
    "cuda_requirement": "TARGET.CUDACapability >= 6.0",
    "bid": 20,
    "forbidden_hostnames": []
  },
  "fixed_params": {
    "__import_promise__": "solo_legged_gym/envs/solo12_sac/solo12_sac.json"
  },

  "remove_jobs_dir": false,
  "run_in_working_dir": true,
  "local_run": false,
  "restarts": 1,
  "hyperparam_list": [
    {
     "param": ["solo_legged_gym.train_cfg.algorithm.qvalues_optimizer_lr", "solo_legged_gym.train_cfg.algorithm.policy_optimizer_lr"],
     "values": [[3e-3, 1e-3], [5e-4, 1e-4], [1e-3, 5e-4]]
    },
    {
     "param": "solo_legged_gym.train_cfg.algorithm.gamma",
     "values": [0.9, 0.95, 0.99]
    },
    {
     "param": "solo_legged_gym.train_cfg.algorithm.tau",
     "values": [0.005, 0.01, 0.05]
    },
    {
     "param": "solo_legged_gym.train_cfg.algorithm.num_mini_batches",
     "values": [200, 500, 1000]
    }
  ]
}
