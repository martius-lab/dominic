{
  "optimization_procedure_name": "test",
  "results_dir": "results",
  "script_relative_path": "solo_legged_gym/cluster/train_cluster.py",
  "git_params": {
  },
  "environment_setup": {
    "run_as_module": false
  },
  "cluster_requirements": {
    "request_cpus": 1,
    "request_gpus": 1,
    "memory_in_mb": 5000,
    "cuda_requirement": "TARGET.CUDACapability >= 6.0",
    "bid": 20,
    "forbidden_hostnames": []
  },
  "fixed_params": {
    "__import_promise__": "solo_legged_gym/envs/solo12_domino/solo12_domino.json"
  },

  "remove_jobs_dir": false,
  "run_in_working_dir": true,
  "local_run": false,
  "restarts": 1,
  "hyperparam_list": [
    {
     "param": "solo_legged_gym.train_cfg.algorithm.alpha",
     "values": [0.5, 0.6, 0.7, 0.8, 0.9]
    },
    {
     "param": "solo_legged_gym.train_cfg.algorithm.burning_expert_steps",
     "values": [100, 300, 500, 600000]
    },
    {
     "param": "solo_legged_gym.train_cfg.algorithm.avg_values_decay_factor",
     "values": [0.9, 0.95, 0.99]
    }
  ]
}
