{
  "optimization_procedure_name": "test",
  "results_dir": "results",
  "script_relative_path": "solo_legged_gym/cluster/train_cluster.py",
  "git_params": {
  },
  "environment_setup": {
    "run_as_module": false
  },
  "cluster_requirements": {
    "request_cpus": 2,
    "request_gpus": 1,
    "memory_in_mb": 10000,
    "cuda_requirement": "TARGET.CUDACapability >= 6.0",
    "bid": 15,
    "forbidden_hostnames": []
  },
  "fixed_params": {
    "__import_promise__": "solo_legged_gym/envs/solo12_sac/solo12_sac.json"
  },

  "remove_jobs_dir": false,
  "run_in_working_dir": true,
  "local_run": false,
  "restarts": 1,
  "hyperparam_list": [
    {
     "param": "solo_legged_gym.train_cfg.algorithm.policy_optimizer_lr",
     "values": [1e-5, 5e-5, 1e-4, 5e-4, 1e-3]
    },
    {
     "param": "solo_legged_gym.train_cfg.algorithm.qvalues_optimizer_lr",
     "values": [1e-5, 5e-5, 1e-4, 5e-4, 1e-3]
    },
    {
     "param": "solo_legged_gym.train_cfg.algorithm.ent_coef_optimizer_lr",
     "values": [1e-5, 1e-4, 1e-3]
    }
  ]
}
